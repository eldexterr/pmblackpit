            SetConst  *Var[0]  00000001
            SetConst  *Var[1]  004E000C
            ExecWait  DoDeath
            Return
        Case  ==  .Event:SpinSmashHit % B
            SetConst  *Var[0]  00000001
            SetConst  *Var[1]  004E000A
            ExecWait  DoSpinSmashHit
