% % Remove the Hand Cursor when inside the Map Tab
@Hook 136884 % 80243544
{
% Check if the cursor is inside the Extra Tab
LAW t0, $subMenuInMapTab
ORI t1, r0, .achievementsTabValue
BEQ t0, t1, .BypassCursor
NOP
% LABU    T0, 802700D4 % Current Opened tab, 00=None, 06=Extra Tab
% ADDIU   T1, R0, 6
% BEQ     T0, T1, .BypassCursor
% NOP
% Reset Start Timer for the Extra Tab
.forceDrawCursor
LIO     T0 $ExtraTab_CursorData3
SB      R0, 3 (T0)
JAL	   801447E0 % Draw Hand Cursor
LW	   A0, 0 (S0)
% Check if the Start timer has been already initialized
J       80243550
NOP
.BypassCursor
% I know it has the same jump twice, I just didn't wanted
% the game to always check the timer on the pause menu
LIO     T0 $ExtraTab_CursorData3
LBU     T1, 3 (T0) % Load Init Timer
SLTI    T2, T0, 4
BNE     T1, R0, .AumentInitTimer
ADDIU   T1, R0, 4 % Freeze the timer in 4
ADDIU   T1, T1, 1
.AumentInitTimer
SB      T1, 3 (T0)
J       80243550
NOP
}

