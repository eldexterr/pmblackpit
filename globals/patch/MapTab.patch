.SetSkipperFlag_Right
LIO 	     A1, *Flag_DisableSkipper
JAL 	     ~Func:set_variable
RESERVED
% Play Sound
BEQ       R0, R0, .SetSettingsFlag_PlaySoundFX_Accept
NOP
.GetShorctusFlag_Right
ADDIU     T3, R0, 5
BNE       T1, T3, .GetGoldenFlag_Right
NOP
LIO 	     A1, *Flag_DisableShortcuts
JAL 	     ~Func:get_variable 
RESERVED
% The flag is true?
BNE       V0, R0, .End
NOP
ADDIU     A2, R0, 1 % Set as true
.SetShorcutsFlag_Right
LIO 	     A1, *Flag_DisableShortcuts
JAL 	     ~Func:set_variable
RESERVED
% Play Sound
BEQ       R0, R0, .SetSettingsFlag_PlaySoundFX_Accept
NOP
.GetGoldenFlag_Right
