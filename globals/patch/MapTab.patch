JAL 	     ~Func:get_variable 
RESERVED
% The flag is false?
BEQ       V0, R0, .End
NOP
ADDIU     A2, R0, 0 % Set as False
.SetGoldenFlag_Right
LIO 	     A1, *Flag_GoldenLS
JAL 	     ~Func:set_variable
RESERVED
% Play Sound
BEQ       R0, R0, .SetSettingsFlag_PlaySoundFX_Accept
NOP
