% % Enlarge Map BG Impression Area
@Hook 1410BC % 8024DD7C
{
ADDIU	V1, R0, 0   % Print Area - Y Pos
ADDIU	T7, R0, 130 % Print Area - Y Size
ADDIU	A0, R0, 20  % Print Area - X Pos
ADDIU	A2, R0, 130 % Print Area - X Size
J		8024DD84
SW		V0, 4 (A1)
}

% % To only leave the Extra Tab if the player is at the start Menu
% @Hook 141F40 % 8024EC00
% {
% LIO       T0, $ExtraTab_CursorData
% LBU       T0, 1 (T0) % Load Current Menu
% BNE       T0, R0, .DontLeaveExtraTab
% NOP
% SAB       R0, 802700D4
% .DontLeaveExtraTab
% J         8024EC08
% NOP
% }

% % To reset the Extra Menu Data everytime you open the pause menu
@Hook FE94 % 80034A94
{
PUSH      RA
JAL	     8005AEA0
SB	     S1, 0070 (V0)
POP       RA
SAW       R0, $ExtraTab_CursorData
SAW       R0, $ExtraTab_CursorData2
SAW       R0, $ExtraTab_CursorData3
SAW       R0, $ExtraTab_CursorData4
J         80034A9C
NOP
}


