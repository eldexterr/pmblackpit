%====================
% Joystick Navigation
%===================
% Set New Y Position for the cursor
% T2= 0 = Up
% T2= 1 = Down
% S1= Current Menu ID
% S5= Current Menu Columns
% S7= Cursor Current Y Pos
.CursorPos_ChangeYPos
LIO       T0, $ExtraTab_CursorData4 % Nuke "Go back to Main Menu?" confirmation bool
SB        R0, 1 (T0)
BNE		T2, R0, .MoveCursor_Down
NOP
% Check if Menu5 is the current one
ADDIU     T0, R0, 5
BNE       S1, T0, .MoverCursor_Up_BypassMenu5_Checks
NOP
% Check if the cursor is on the last pos
ORI       T0, R0, 6 % last pos
BNE       S7, T0, .MoverCursor_Up_BypassGoldenLS_1
NOP
% Check if the Golden LS has been unlocked
LABU      V0, $ExtraTab_CursorData4
% The cursor is on the lowest position and the golden ls isn't unlocked
BEQL      V0, R0, .SaveCursorPos
ORI       S7, R0, 4
.MoverCursor_Up_BypassGoldenLS_1
% Check if the cursor is on the top position
BNE       S7, R0, .MoverCursor_Up_JustMoveUp
NOP
BEQ       R0, R0, .SaveCursorPos
ADDIU     S7, R0, 6 % set the cursor to the lowest column
% For all other Menus
% Just move the cursor upwards
.MoverCursor_Up_BypassMenu5_Checks
% Check if the cursor is on the top column
BEQL	     S7, R0, .SaveCursorPos
ADDIU     S7, R0, 4 % set the cursor to the lowest column
% Just move the cursor Upwards
.MoverCursor_Up_JustMoveUp
ORI       T0, R0, 1
BEQ		R0, R0, .SaveCursorPos
SUBU	     S7, S7, T0
.MoveCursor_Down
% Check if Menu5 is the current one
ADDIU     T0, R0, 5
BNE       S1, T0, .MoverCursor_Down_BypassMenu5_Checks
NOP
% Check if the cursor is on the last pos
ORI       T0, R0, 6 % last pos
BEQL      S7, T0, .SaveCursorPos
CLEAR     S7 % set the cursor to the 1st column
% The cursor is on position 4?
ORI       T0, R0, 4
BNE       S7, T0, .MoverCursor_Down_JustMoveDown
NOP
% Check if the Golden LS has been unlocked
LABU      V0, $ExtraTab_CursorData4
BEQL      V0, R0, .SaveCursorPos
ORI       S7, R0, 6 % lowest pos
BEQ       R0, R0, .MoverCursor_Down_JustMoveDown
NOP
% For all other Menus
.MoverCursor_Down_BypassMenu5_Checks
% Check if the cursor is on the lowest position
ADDIU     T0, R0, 4 % lowest column (except menu5/settings)
BEQL	     S7, T0, .SaveCursorPos
CLEAR     S7 % set the cursor to the 1st column
% Just move the cursor downwards
.MoverCursor_Down_JustMoveDown
BEQ		R0, R0, .SaveCursorPos
ADDIU	S7, S7, 1
