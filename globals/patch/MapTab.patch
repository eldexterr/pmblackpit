%=================
% Cursor Inputs
%=================
/% 80074035 = Joystick (1 Frame Input)
bit0=Up
bit1=Down
bit2=Left
bit3=Right

80074036 = Inputs 1
bit5=Z Button
bit6=B Button
bit7=A Button

80074037 = Inputs 2
bit4=R Button
bit5=L Button
%/
% Track Joystick vertical movement
% Up
.TrackInputs
LABU      T0, 80074035 % Joystick (1 Frame Input)
COPY      T1, T0
SLL	     T1, T1, 31`
SRL 	     T1, T1, 31` % Move all bits to just leave bit0
BNEL	     T1, R0, .CursorPos_ChangeYPos % If was indeed moved upwards
ADDIU	T2, R0, 0
% Down
COPY      T1, T0
SLL		T1, T1, 30`
SRL 	     T1, T1, 31` % Move all bits to just leave bit1
BNEL	     T1, R0, .CursorPos_ChangeYPos % If was indeed moved downwards
ADDIU	T2, R0, 1
%=
% Track Joystick horizontal movement
%=
COPY      T1, T0
SLL		T1, T1, 29`
SRL 	     T1, T1, 31` % Move all bits to just leave bit2
BNEL	     T1, R0, .CursorPos_ChangeXPos % If was indeed moved to the left
ADDIU	T2, R0, 0
% Down
COPY      T1, T0
SLL		T1, T1, 28`
SRL 	     T1, T1, 31` % Move all bits to just leave bit3
BNEL	     T1, R0, .CursorPos_ChangeXPos % If was indeed moved downwards
ADDIU	T2, R0, 1
%=
% Track Buttons
%=
LABU      T0, 80074036 % Inputs 1 / bit5=Z, bit6=B, bit7=A
% A Button
COPY      T1, T0
SLL		T1, T1, 24`
SRL 	     T1, T1, 31` % Move all bits to just leave bit7
BNEL	     T1, R0, .MenuNavigation_Inputs % If the button was indeed pressed
ADDIU	T2, R0, 2
% B Button
COPY      T1, T0
SLL		T1, T1, 25`
SRL 	     T1, T1, 31` % Move all bits to just leave bit6
BNEL	     T1, R0, .MenuNavigation_Inputs % If the button was indeed pressed
ADDIU	T2, R0, 3
% Z Button
COPY      T1, T0
SLL		T1, T1, 26`
SRL 	     T1, T1, 31` % Move all bits to just leave bit5
BNEL	     T1, R0, .MenuNavigation_Inputs % If the button was indeed pressed
ADDIU	T2, R0, 4
% L Button
LABU      T0, 80074037 % Inputs 2 / bit4=R, bit5=L
COPY      T1, T0
SLL		T1, T1, 26`
