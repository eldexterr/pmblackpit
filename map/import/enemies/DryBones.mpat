#new:Script $Script_DryBones_Projectile_Main
{
    Call     $Function_DryBones_Projectile_1
    If  *Var[0]  ==  0
        Return
    EndIf
    Call     BindNpcAI   	( .Npc:Self $Script_null )
    Call     GetOwnerEncounterTrigger ( *Var[0] )
    Switch  *Var[0] 
        CaseOR  ==  .EncounterTrigger:Hammer % 4
        CaseOR  ==  .EncounterTrigger:Spin % 3
            Call     SetSelfVar 	( 00000000 00000003 )
            Call     $Function_DryBones_Projectile_2
            If  *Var[0]  ==  0
                Return
            EndIf
        EndCaseGroup
        CaseOR  ==  .EncounterTrigger:Jump % 2
        CaseOR  ==  .EncounterTrigger:Partner % 6
            Call     SetSelfVar 	( 00000000 00000004 )
            Call     GetNpcPos   	( .Npc:Self *Var[0] *Var[1] *Var[2] )
            Call     PlayEffect  	( ~FX:Steam:Ring *Var[0] *Var[1] *Var[2] 00000000 00000000 00000000 00000000 00000000 00000000 00000000 00000000 00000000 )
            Call     SetNpcPos   	( .Npc:Self 0 -1000` 0 )
            Call     SetSelfVar 	( 00000000 00000000 )
        EndCaseGroup
        Default
            Call     800457F8
        EndCaseGroup
    EndSwitch
    Call     BindNpcAI   	( .Npc:Self $Script_ProjectileAI )
    Return
    End
}
  
