% Used if you want to use a different Npc ID
% for the Projectiles NPCs
#new:Function $Func_FormatNpcID
{
      % A0 = NpcID
      ADDIU T0, R0, 10 % Projectile 1 - New NPC ID
      BEQL  A0, T0, .Jump
      ORI   A0, R0, 1
      ADDIU T0, R0, 11 % Projectile 2
      BEQL  A0, T0, .Jump
      ORI   A0, R0, 2
      ADDIU T0, R0, 12 % Projectile 3
      BEQL  A0, T0, .Jump
      ORI   A0, R0, 3
      .Jump
      JR    RA
      ORI   V0, R0, 2
}

