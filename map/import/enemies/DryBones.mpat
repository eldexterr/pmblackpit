#new:Script $Script_ProjectileOutcome
{
    Call  GetBattleOutcome  ( *Var[0] )
    Switch  *Var[0]
        Case  ==  0
            Call  SetSelfVar    ( 0 5 )
            Call  RemoveNpc     ( .Npc:Self )
        Case  ==  2
            Call  SetNpcPos ( .Npc:Self 0 -1000` 0 )
            Call  80045900  ( 1 )
        Case  ==  3
            Call  SetEnemyFlagBits  ( .Npc:Self 00000010 .True )
            Call  RemoveNpc ( .Npc:Self )
    EndSwitch
    Return
    End
}

#new:Script $Script_DryBonesAI
{
    Call     SetSelfVar 	( 00000000 00000000 )
    Call     SetSelfVar 	( 00000001 0000000F )
    Call     SetSelfVar 	( 00000002 0000000A )
    Call     SetSelfVar 	( 00000003 00000002 )
    Call     $Function_DryBones_Main ( $AISettings_DryBones )
    Return
    End
}
   
   #new:Script $Script_ProjectileAI
{
    Call     SetSelfVar 	( 00000000 00000000 )
    Call     SetSelfVar 	( 00000001 00000003 )
    Call     SetSelfVar 	( 00000002 0000000F )
    Call     SetSelfVar 	( 00000003 0000000F )
    Call     $Function_DryBones_Projectile_Main ( $AISettings_Projectile )
    Return
    End
}
	
